<table class="table table-dark table-hover">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Surname</th>
            <th scope="col">Age</th>
            <th scope="col">Country</th>
            <th scope="col">PhoneNumber</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <th scope="row">1</th>
            <td>@ViewBag.Name1</td>
            <td>@ViewBag.Surname1</td>
            <td>@ViewBag.Age1</td>
            <td>@ViewBag.Country1</td>
            <td>@ViewBag.PhoneNumber1</td>
        </tr>
        <tr>
            <th scope="row">2</th>
            <td>@ViewBag.Name2</td>
            <td>@ViewBag.Surname2</td>
            <td>@ViewBag.Age2</td>
            <td>@ViewBag.Country2</td>
            <td>@ViewBag.PhoneNumber2</td>
        </tr>
        <tr>
            <th scope="row">3</th>
            <td>@ViewBag.Name3</td>
            <td>@ViewBag.Surname3</td>
            <td>@ViewBag.Age3</td>
            <td>@ViewBag.Country3</td>
            <td>@ViewBag.PhoneNumber3</td>
        </tr>
    </tbody>
</table>